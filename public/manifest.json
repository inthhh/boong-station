{
  "manifest_version": 3,
  "name": "붕어빵 스테이션",
  "version": "1.0",
  "description": "투두리스트, 일정 관리, 메모와 시계를 포함한 생산성 대시보드",
  "icons": {
    "16": "boong16.png",
    "32": "boong32.png",
    "48": "boong48.png",
    "128": "boong128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"],
  "host_permissions": ["<all_urls>"]
}
