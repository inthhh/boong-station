{
  "manifest_version": 3,
  "name": "붕어빵 스테이션 - Chrome Dashboard",
  "version": "1.2",
  "description": "당신의 크롬 시작 화면을 귀엽게! Bookmarks, todoList, Clock을 포함한 생산성 대시보드",
  "icons": {
    "16": "boong16.png",
    "32": "boong32.png",
    "48": "boong48.png",
    "128": "boong128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["bookmarks"],
  "host_permissions": ["<all_urls>"]
}
